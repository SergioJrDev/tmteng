.dashborad {
	&-view {
		background: #f5f5f5;
		padding: 15px;
	}
	&-menu {
		a.active {
		    background: #656565;
		    padding: 5px;
		    color: #fff;
		}
		li {
			width: 100%;
		}
		a {
			display: block;
			margin-bottom: 5px;
		}
	}

	@media (min-width: sm) {
		&-content {
			display: flex;
		}
		&-menu {
			
			li {
				padding: 5px 10px 5px 0;
			}
			a {
				display: block;
			}
		}
		&-view {
			
		}
	}
}